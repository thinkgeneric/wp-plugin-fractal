{
  "name": "thinkgeneric/wp-plugin-fractal",
  "description": "Plugin that allows for importing business listings using the Fractal API.",
  "type": "wordpress-plugin",
  "require": {
    "composer/installers": "1.0.*",
    "webdevstudios/cmb2": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "5.3.*",
    "squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "0.10.0",
    "10up/wp_mock": "dev-dev"
  },
  "autoload": {
    "psr-4": {
      "Fractal\\": "lib"
    }
  },
  "extra": {
    "installer-name": "fractal",
    "installer-paths": {
      "vendor/{$name}/": ["webdevstudios/cmb2","mustardBees/cmb-field-select2"]
    }
  }
}
